RewriteEngine On    # Turn on the rewriting engine

RewriteRule ^mailchimp/([0-9a-zA-Z_-]*)$ mailchimp/index.php?token=$1 [NC,L]
RewriteRule ^mailchimp/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)?$ mailchimp/index.php?token=$1&arg1=$2 [NC,L]
RewriteRule ^mailchimp/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)?$ mailchimp/index.php?token=$1&arg1=$2&arg2=$3 [NC,L]
RewriteRule ^mailchimp/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)?$ mailchimp/index.php?token=$1&arg1=$2&arg2=$2&arg3=$3 [NC,L]
RewriteRule ^mailchimp/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)?$ mailchimp/index.php?token=$1&arg1=$2&arg2=$2&arg3=$3&arg4=$4 [NC,L]

RewriteRule ^dropbox/([0-9a-zA-Z_-]*)$ dropbox/index.php?token=$1 [NC,L]
RewriteRule ^dropbox/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)?$ dropbox/index.php?token=$1&arg1=$2 [NC,L]
RewriteRule ^dropbox/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)?$ dropbox/index.php?token=$1&arg1=$2&arg2=$3 [NC,L]
RewriteRule ^dropbox/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)?$ dropbox/index.php?token=$1&arg1=$2&arg2=$2&arg3=$3 [NC,L]
RewriteRule ^dropbox/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)/([0-9a-zA-Z_-]*)?$ dropbox/index.php?token=$1&arg1=$2&arg2=$2&arg3=$3&arg4=$4 [NC,L]